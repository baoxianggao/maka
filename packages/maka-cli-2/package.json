{
  "name": "@makajs/cli2",
  "version": "2.0.0",
  "description": "makajs cli工具新版本，基于common-bin",
  "main": "index.js",
  "bin": {
    "maka2": "bin/maka-cli.js",
    "mocha": "bin/mocha.js"
  },
  "dependencies": {
    "@types/mocha": "^5.0.0",
    "autod": "^3.1.0",
    "babel-preset-airbnb": "^1.0.1",
    "co-mocha": "^1.2.2",
    "coffee": "^5.2.1",
    "common-bin": "^2.8.1",
    "compressing": "^1.5.0",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "enzyme": "^2.0.0",
    "eslint": "^4.12.1",
    "eslint-config-maka": "^1.0.1",
    "espower-typescript": "9.0.1",
    "globby": "^9.2.0",
    "intelli-espower-loader": "^1.0.1",
    "istextorbinary": "^3.3.0",
    "jest-changed-files": "^24.7.0",
    "jsdom": "^8.0.1",
    "mm": "^2.5.0",
    "mocha": "^6.0.2",
    "mz-modules": "^2.1.0",
    "node-homedir": "^1.1.1",
    "npm-updater": "^3.0.3",
    "nyc": "^13.3.0",
    "power-assert": "^1.6.1",
    "rc": "^1.2.8",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "test-exclude": "^5.1.0",
    "ts-node": "^7",
    "typescript": "^3",
    "ypkgfiles": "^1.6.0"
  },
  "devDependencies": {},
  "scripts": {
    "lint": "eslint .",
    "maka-lint": "cross-env DEBUG=maka-cli node bin/maka-cli.js lint",
    "maka-cov": "cross-env DEBUG=maka-cli node bin/maka-cli.js cov",
    "pkgfiles": "node bin/maka-cli.js pkgfiles",
    "cov": "nyc -r lcov -r text-summary npm run test-local",
    "cov-debug": "cross-env DEBUG=maka-cli nyc -r lcov -r text-summary npm run test-local",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "node bin/maka-cli.js test -t 3600000",
    "test-local-debug": "cross-env DEBUG=maka-cli node bin/maka-cli.js test ",
    "autod": "node bin/maka-cli.js autod",
    "ci-test-only": "TESTS=test/lib/cmd/cov.test.js npm run test-local",
    "ci-test-only-debug": "DEBUG=maka-cli TESTS=test/lib/cmd/cov.test.js npm run test-local",
    "ci": "npm run lint && npm run pkgfiles -- --check && npm run ci-test-only && npm run cov"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "index.js",
    "lib",
    "bin"
  ]
}
